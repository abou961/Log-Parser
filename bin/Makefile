analog: analog.o Node.o Structure.o InterfaceAnalogue.o
	g++ analog.o Node.o Structure.o InterfaceAnalogue.o -o analog  -g

analog.o: analog.cpp 
	g++ -c analog.cpp -g

InterfaceAnalogue.o: InterfaceAnalogue.cpp
	g++ -c InterfaceAnalogue.cpp -g

Structure.o: Structure.cpp
	g++ -c Structure.cpp -g

Node.o: Node.cpp
	g++ -c Node.cpp -g


